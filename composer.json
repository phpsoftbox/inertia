{
    "name": "phpsoftbox/inertia",
    "description": "Inertia.js adapter for the PhpSoftBox framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton K.",
            "email": "inspector-who@mail.ru"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.4",
        "phpsoftbox/session": "dev-master",
        "phpsoftbox/view": "dev-master",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.93",
        "phpsoftbox/cli-app": "dev-master",
        "phpsoftbox/cs-fixer": "^1.0.4",
        "phpsoftbox/http-message": "dev-master",
        "phpunit/phpunit": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "PhpSoftBox\\Inertia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSoftBox\\Inertia\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs:fix": "vendor/bin/psb-cs-fixer fix",
        "cs:check": "vendor/bin/psb-cs-fixer check --diff"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
